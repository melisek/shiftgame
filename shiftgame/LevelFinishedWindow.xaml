    <Grid>
        <Label x:Name="labelLevelCompletedTitle" Content="" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Width="272" Height="42" FontSize="16" FontFamily="Arial" FontWeight="SemiBold" Foreground="White" HorizontalContentAlignment="Center" IsManipulationEnabled="True"/>
        <Label x:Name="labelLevelCompletedTime" Content="" HorizontalAlignment="Center" Margin="18,55,154,0" VerticalAlignment="Top" FontSize="16" FontFamily="Arial" FontWeight="SemiBold" Width="136" Foreground="White" IsHitTestVisible="False"/>
        <Label x:Name="labelLevelCompletedSteps" Content="" HorizontalAlignment="Center" Margin="162,55,18,0" VerticalAlignment="Top" FontSize="16" FontFamily="Arial" FontWeight="SemiBold" Width="128" Foreground="White" IsHitTestVisible="False"/>
        <Label Content="Név" HorizontalAlignment="Center" Margin="18,93,250,0" VerticalAlignment="Top" FontSize="16" FontFamily="Arial" FontWeight="SemiBold" Width="40" Foreground="White" IsHitTestVisible="False"/>
