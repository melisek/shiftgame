        <Button Content="Következő pálya" HorizontalAlignment="Left" Margin="18,142,0,0" VerticalAlignment="Top" Width="150" Background="White" FontSize="16" FontFamily="Arial" FontWeight="SemiBold" Click="Button_Click"/>
        <Button Content="Menü" HorizontalAlignment="Left" Margin="223,142,0,0" VerticalAlignment="Top" Width="58" Background="White" FontSize="16" FontFamily="Arial" FontWeight="SemiBold" Click="Button_Click_1"/>
    </Grid>
</Window>
